package javac;

import java.util.LinkedList;
import java.util.Scanner;

public class Lname_LinkedList {
	public static void main (String[]args){
		LinkedList<String> myFlavors =new LinkedList<String>();
		myFlavors.add("Green Reds Soda");
		myFlavors.add("Bubble Gum Mint");
		myFlavors.add("Peach Mango Shake");
		myFlavors.add("Ice Coffee Cream");
		System.out.println(myFlavors);
	}
}
